!set title= Le carré d'un entier avec 2 essais 
!if $wims_read_parm!=$empty
 !goto $wims_read_parm
!endif
oef2wims_version=3.22
computeanswer=no

ansorder=r,r
varcnt=7
prevarcnt=4
postvarcnt=2
replycnt=2
choicecnt=0
conditioncnt=1
feedbackcnt=3
format=html

val1=$imagedir
val2=$confparm1
val3=$confparm2
val4=$confparm3
val5=$confparm4

!if $testcondition=yes
 !goto test
!endif
!if $status=waiting
 !exit
!endif

tmp0=!randint -5, 5

val6=$[rint($(tmp0) )]
val7=$[rint(($val6)^2 )]
val8=1
val9=r1
dynsteps=yes
oefsteps=$val9 
nextstep=!nosubst $val9 
!readproc oef/steps.proc
replytype1=numeric
replyname1=Carré de $val6
replygood1=?analyze 10
replytype2=numeric
replyname2=Carré de $val6 (2ème essai)
replygood2=$[$val7 ]
question=__EXECUTED_1692754_EXERCISE__

!goto stat

!exit

:question
$() Calculer le carré de $val6.<br>	
$()
!exit

:hint
$()
$()
!exit

:help
$()
$()
!exit

:solution
$()
$()
!exit

:postdef
val9=
!if $m_step = 2 and $val10!=$val7 
val9=r2
!endif

!exit

:feedback
!ifval  $m_reply1 < 0 
 <p> Le carré d'un entier est toujours positif. 
$()
!endif
!ifval $val10!=$val7 and $m_reply2 = $val7
 <p>	    Vous avez répondu correctement au 2ème essai.	
$()
!endif
!ifval $val10!=$val7 and $m_reply2!=$val7
 <p>Vos deux réponses sont incorrectes.
$()
!endif

!exit

:test

condlist=1

!ifval $val10 = $val7
 condtest1=1
!else
 condtest1=0
!endif
condname1=Votre première réponse est-elle correcte ?

!exit

:stat
vsavelist=6,7,10
embedcnt=0
