val11=$[$(tmp0)]
tmp=!translate ;";': to $     $ in $val9,x

tmp=!lower $tmp
tmp0=!exec maxima diff($tmp);
val12=!rawmath $(tmp0)
evaltmp=$val12

evaltmp=!mathsubst x=( $val10) in $evaltmp
tmp0=($evaltmp)
val13=$[$(tmp0)]
tmp=!translate ";': to $     $ in $val13*$val10-$val11

tmp=!lower $tmp
tmp0=!exec maxima fullratsimp($tmp);
val14=!rawmath $val13*x-$(tmp0)
replytype1=function
replyname1=y
replygood1=!rawmath $val14,x
question=__EXECUTED_1692754_EXERCISE__

