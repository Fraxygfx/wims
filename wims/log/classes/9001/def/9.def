tmp0=!randitem -1,1

tmp1=!randint 1, 30

val6=$[$(tmp0)*$(tmp1)/10]
tmp0=!randint -20, 20

val7=$[$(tmp0)/10]
tmp0=!randint -20, 20

val8=$[$(tmp0)/10]
