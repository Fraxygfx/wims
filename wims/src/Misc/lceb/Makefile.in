# @configure_input@

wims_home=../../..
cc=@CC@
cflags=@CFLAGS@ -Wall
defines=@DEFINES@
lopt=-lm

LDFLAGS =

all: lceb_dynopt lceb_dyn lceb_rec lceb_bigsum lceb_easiest

clean:
	rm -f *.o *.core *.stackdump lceb_dynopt lceb_dyn lceb_rec random lceb_bigsum lceb_easiest

lceb_dynopt: lceb_dynopt.c
	$(cc) $(cflags) $(LDFLAGS) -o lceb_dynopt lceb_dynopt.c

lceb_dyn: lceb_dyn.c
	$(cc) $(cflags) $(LDFLAGS) -o lceb_dyn lceb_dyn.c

lceb_rec: lceb_rec.c
	$(cc) $(cflags) $(LDFLAGS) -o lceb_rec lceb_rec.c

random: random.c
	$(cc) $(cflags) $(LDFLAGS) -o random random.c

lceb_bigsum: lceb_bigsum.c
	$(cc) $(cflags) $(LDFLAGS) -o lceb_bigsum lceb_bigsum.c

lceb_easiest: lceb_easiest.c
	$(cc) $(cflags) $(LDFLAGS) -o lceb_easiest lceb_easiest.c
