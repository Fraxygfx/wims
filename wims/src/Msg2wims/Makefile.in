# @configure_input@

wims_home=../..
cc=@CC@
cflags=@CFLAGS@ -Wall
defines=@DEFINES@
STRIP=@STRIP@
rpath=
libtype=static
lopt=-L../../lib -lwims -lm $(rpath)

all: msg2wims

clean:
	rm -f msg2wims 2>/dev/null

distclean:
	rm -f msg2wims $(wims_home/bin/msg2wims $(wims_home)/public_html/bin/msg2wims 2>/dev/null
	rm -f Makefile

msg2wims: *.c ../Oef2wims/sp.c ../Lib/libwims.*
	$(cc) $(cflags) $(defines) msg2wims.c $(lopt) -o msg2wims
	./msg2wims table
	$(STRIP) msg2wims
	cp msg2wims $(wims_home)/bin
