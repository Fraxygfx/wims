!if $wims_read_parm!=slib_header
!goto proc
!endif
slib_title=Quantile of Weibull law
slib_parms=3\
,[data]\
,a\
,b

slib_author=Bernadette PERRIN-RIOU
slib_out=quantiles at data
slib_comment=data can be a list of reals in brackets; density is a*b*exp(-b*x^a)*x^(a-1) for x&gt;0
slib_example=[0.2,0.4,0.7],6,8
slib_require=octave
!exit

:proc
slib_out=

!distribute  item $wims_read_parm into slib_data,slib_a, slib_b

slib_data=!declosing $slib_data
slib_c=$[$slib_b^(-$slib_a)]

slib_out=!exec octave weibull_inv([$slib_data], $slib_a,$slib_c)

slib_out=!words2items $slib_out

slib_out=!trim $slib_out

