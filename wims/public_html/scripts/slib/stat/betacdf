!if $wims_read_parm!=slib_header
!goto proc
!endif
slib_title=Cumulative density function of Beta law
slib_parms=3\
,[data]\
,a\
,b

slib_author=Bernadette PERRIN-RIOU
slib_out=values on data
slib_comment=data can be a list of reals in brackets; 
slib_example=[0.4,0.5,0.9],3,4
slib_require=octave
!exit

:proc
slib_out=

!distribute  item $wims_read_parm into slib_data,slib_a,slib_b

slib_data=!declosing $slib_data

slib_out=!exec octave beta_cdf([$slib_data],$slib_a,$slib_b)
slib_out=!words2items $slib_out
slib_out=!trim $slib_out

