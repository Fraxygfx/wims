
!if $wims_read_parm!=slib_header
!goto proc
!endif
slib_title=Quantile of of Log-normal law
slib_parms=3\
,[data]\
1,mean\
1,ecart-type

slib_author=Bernadette PERRIN-RIOU
slib_out=quantile at data
slib_comment=data can be a list of reals.
 slib_example=[0.2,0.4,0.7] ,3,4
 !exit

:proc
slib_out=
!distribute  item $wims_read_parm into slib_data,slib_mean,slib_var
!default slib_mean=0
!default slib_var=1
slib_data=!declosing $slib_data

slib_out=!exec octave lognormal_inv([$slib_data], $[exp($slib_mean)],$[$slib_var^2])
slib_out=!words2items $slib_out
slib_out=!trim $slib_out

