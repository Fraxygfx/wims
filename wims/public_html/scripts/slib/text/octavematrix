!if $wims_read_parm!=slib_header
 !goto proc
!endif
slib_title=Transform an octave output matrix into standard format
slib_parms=1\
,output matrix from octave.\
slib_author=Bernadette Perrin-Riou
slib_out=the output matrix in format wims from output of Octave.
!exit

:proc

slib_out=!trim $wims_read_parm
slib_out=!rows2lines $slib_out
slib_out=!replace internal $\
$ by ; in $slib_out
slib_out=!words2items $slib_out

