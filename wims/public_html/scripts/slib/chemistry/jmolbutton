!if $wims_read_parm!=slib_header
 !goto proc
!endif
slib_title=Button in Jmol
slib_parms=3\
,Jmol script\
OK,text\
,id (optional)
slib_author=Yves Noël et Bernadette Perrin-Riou
slib_out=applet code for inserting jmol button
slib_comment=

!!slib_example=select all;color atoms blue; select none\
bleu
!exit

:proc
!distribute items $wims_read_parm into slib_script,slib_text,slib_id
!if $slib_id!=
  slib_id=,'$slib_id'
!endif
slib_out=<script language="javascript" type="text/javascript">\
    jmolButton('$slib_script','$slib_text'$slib_id);\
</script>