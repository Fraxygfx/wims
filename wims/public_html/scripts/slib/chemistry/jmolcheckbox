!if $wims_read_parm!=slib_header
 !goto proc
!endif
slib_title=Checkbox Button in Jmol
slib_parms=6\
,jmol script when checked\
,jmol script when unchecked\
,label\
,true or false (checked at the start)\
,id (optional)\
,title (optional)

slib_author=Yves Noël et Bernadette Perrin-Riou
slib_out=applet code for inserting jmol applet
slib_comment=

slib_example=select all;color atoms blue; select none\
bleu
!exit

:proc
!distribute items $wims_read_parm into slib_scriptWhenChecked, slib_scriptWhenUnchecked, slib_labelHtml, slib_isChecked, slib_id, slib_title
!!if $slib_id!=
!!!!  slib_id=,'$slib_id'
!!!endif

slib_out=<script language="javascript" type="text/javascript">\
jmolCheckbox('$slib_scriptWhenChecked', '$slib_scriptWhenUnchecked', '$slib_labelHtml', '$slib_isChecked', '$slib_id','$slib_title')  \
</script>