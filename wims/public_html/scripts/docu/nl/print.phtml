!nocache
!if $error!=$empty
 !read docu/nl/msg.phtml
 <p>
 !exit
!endif

<h1 class="wims_docu_title">$tit</h1>

!if $titb=$empty
 <p>
 Het blok <tt>$block</tt> is nog niet aangemaakt.
 <p>
 !exit
!endif

!if $datetest<0
 <p>
 Deze pagina is nog niet publiek beschikbaar. Kom later maar eens terug !
 <p>
 !exit
!endif

!if $datetest>0
 <p>
 Deze pagina is verlopen, en dus helaas niet meer beschikbaar.
 <p>
 !exit
!endif

:newblock
<a name=$block></a>
<h3>$titb</h3>

!read $docdir/$doc/$block.def content

!if $waitblock$(waitblock_$block)!=$empty
 !set waitblock=$(waitblock_$block),$waitblock
 !set waitblock=!nonempty items $waitblock
 !set block=!item 1 of $waitblock
 !set gotblock=!append item $block to $gotblock
 !set waitblock=!item 2 to -1 of $waitblock
 !read $docdir/$block.def
 <p>
 !goto newblock
!endif

