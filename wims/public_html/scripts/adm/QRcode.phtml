!distribute item $wims_read_parm into http_,size_
!default size_=50
!let qrencode_=!defof QRencode_command in wimshome/log/wims.conf
!let name_=!word 1 of $qrencode_
!let test_=!sh which $name_
!if /$name_ notin $test_
 !exit
!endif
!sh $qrencode_ $wims_home/s2/$wims_session/qrencode.png $http_
<img src="$wims_ref_name?session=$wims_session&+cmd=getins&+special_parm=qrencode.png" width="$size_" alt="qrencode" />

