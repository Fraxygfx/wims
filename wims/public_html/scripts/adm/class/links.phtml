!if $wims_read_parm=$empty
   !let code=$wims_class
!else
  !let code=$wims_read_parm
!endif
!readproc adm/language adm/lang/links
!readproc adm/lang/links.$modu_lang

$name_link_introduce :
<ul>
 <li>$name_link_link
  <a href="http://$httpd_HTTP_HOST$httpd_SCRIPT_NAME?lang=fr&module=adm/class/classes&type=authsupervisor&class=$code">http://$httpd_HTTP_HOST$httpd_SCRIPT_NAME?lang=$lang&module=adm/class/classes&type=authsupervisor&class=$code</a>
  $name_link_supervisor.
 <p></li>
 <li>$name_link_link
  !set script_out=$wims_ref_name?lang=$lang&module=adm/class/classes&type=authparticipant&class=$code&subclass=yes
  !if $class_auth=cas
   !read adm/codeurl $script_out
  !endif
  <a href="$(link_)$script_out">$(link_)$script_out</a>
  $name_link_user
  !if $Cltype>=2
    $name_link_grpclass.
  !endif
  !if $password!=$empty
    ($name_link_withpwd.).
  !endif
  <br>$name_link_user2.
 <p></li>
 !if $Cltype>=2
  <li>$name_link_link
   !set script_out=$wims_ref_name?lang=$lang&module=adm/class/reguser&class=$code
   !if $class_auth=cas
     !read adm/codeurl $wims_ref_name?lang=$lang&module=adm/class/reguser&class=$code
   !endif
   <a href="$(link_)$script_out">$(link_)$script_out</a>
   $name_link_regsup
   !if $password!=$empty
     ($name_link_withpwd.).
   !else
    .
   !endif
  <p></li>
 !endif
</ul>
<span class="wims_warning">$name_link_important</span> : $name_link_importantmes.
!if adm/class/config notin $module 
 $name_link_importantmes2
!endif
<p>
$name_link_classnumber : <tt class="wims_code_words">$code</tt>.
