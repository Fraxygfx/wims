slib_lang_exists_tmp=yes

slib_title=Simulation de réalisations d'une variable aléatoire suivant la loi binomiale.

slib_out=Simulation de <tt>m</tt> réalisations d'une variable aléatoire suivant la loi binomiale de paramètres <tt>n</tt> et <tt>p</tt> \
(<tt>n</tt> est un entier et <tt>p</tt> un réel entre 0 et 1).
slib_comment= P(X=k)=binomial(n,k)*p^k*(1-p)^(n-k) pour k=0,...,n

