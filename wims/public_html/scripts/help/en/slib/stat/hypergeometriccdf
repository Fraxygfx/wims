slib_lang_exists_tmp=yes

slib_title=Cumulative distribution function of a hypergeometric law

slib_out=Values at <tt>data</tt> of cumulative distribution function of the hypergeometric law\
with parameters <tt>N</tt>, <tt>K</tt> and <tt>n</tt>.
slib_comment=<tt>data</tt> can be a list of reals.\
P(X=i)=binomial(K,i)*binomial(N-K,n-i)/binomial(N,n)

