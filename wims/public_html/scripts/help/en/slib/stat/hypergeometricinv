slib_lang_exists_tmp=yes

slib_title=Quantile of hypergeometric law inverse

slib_out=Quantile at <tt>data</tt> of the hypergeometric law inverse\
with parameters <tt>N</tt>, <tt>K</tt> and <tt>n</tt>.
slib_comment=<tt>data</tt> can be a list of reals;\
P(X=i)=binomial(K,i)*binomial(N-K,i)/binomial(N,n)

