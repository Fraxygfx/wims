
!set wims_html_header=!append line <script language=javascript type="text/javascript">\
var rtick;\
rtick=$wims_read_parm;\
function clock() {\
 document.chrono.clock.value=rtick; rtick--;\
 if(rtick<0) rtick=0;\
 Timer= setTimeout("clock()",1000);\
}\
</script> to $wims_html_header

!set wims_html_onload=!append word clock(); to $wims_html_onload
