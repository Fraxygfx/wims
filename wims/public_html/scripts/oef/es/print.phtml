!!$wims_html_header
!!<body $wims_htmlbody>
!header1
<p><center><h3>
!href module=$module&cmd=intro WIMS
 $module_title<br>
<font size=-1>--- Versión imprimible ---</font></h3></center> <p>
!!<div class=wimsbody>

!if $error!=$empty
 !changeto oef/msg.phtml
!endif

!for i=1 to $n
 <p>
 <b>Ejercicio</b> $i. (<u><em>$(title_$i)</em></u>)
 !if $(question_$i)=$empty
  (No imprimible)
 !else
  !if __EXECUTED_ isin $(question_$i) and _EXERCISE__ isin $(question_$i)
   !set thisexo=$(name_$i)
   !readproc oef/imagedir.proc
   !for k=1 to $(varcnt_$i)+5
    !set val$k=!line $k of $(var_$i)
   !next k
   !read $(class_header)def/$(name_$i).def question
  !else
   $(question_$i)
  !endif
  !!</a></tt></font></center>
 !! </h1></h2></h3></h4></b></i></em>
  $(chce_$i)
 !endif
!next i

!if $wims_user!=$empty and $wims_user!=supervisor
 !goto noans
!endif

<p><hr> 
<div class="wims_solution">
<center><h3>Respuestas</h3></center> <p>
!for i=1 to $n
 !set t=!linecnt $(ans_$i)
 !if $t>0
  <p><b>Respuesta al ejercicio $i.</b><ul>
  !for k=1 to $t
   <li>
   !line $k of $(ans_$i)
  !next k
  </ul>
 !endif
!next i
</div>

:noans
</div>
!!<div class="wims_noprint"><hr>
!!!href cmd=reply&print=yes Otro nuevo
!!&nbsp;&nbsp;
!!!href module=$module&cmd=intro Volver atrás
!!</div>
!!</body></html>
!set wims_menu_items=!append line renew,1,cmd=reply&print=yes \
"Volver atrás",1,module=$module&cmd=intro \
to $wims_menu_items
!tail
