!!## technical parameters

!if $wims_firstname$wims_lastname$wims_user$wims_developer=$empty or _help isin $wims_session
  !exit
!endif
!read themes/_procs/userlinks.proc
!default wims_which_user=xxx
<ul class="wimsmenu float_right dropdown menu" data-dropdown-menu>
    <li class="menuitem">
        <a href="#user_account" class="account">
            <span class>$wims_which_user</span>
        </a>
        <ul id="user_account" class="menu">
    !for i_=1 to $wims_user_links_cnt
         !set 1_=!line $i_ of $wims_user_links
         <li class="menuitem">
                !read oneitem.phtml $1_
         </li>
    !next
       !! "WIMS logout"
         !read themes/_widgets/logout.phtml
       !! "CAS logout"
         !set div1=li
         !read themes/_widgets/authlogout.phtml
      </ul>
    </li>
</ul>

<!-- Foundation Dropdown Menu -->
<script type="text/javascript" src="html/themes/_inc/foundation_dropdown_menu.js"></script>
<script type="text/javascript" >$$(document).foundation()</script>
