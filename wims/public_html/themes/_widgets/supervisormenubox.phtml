<!-- _widgets/supervisormenubox -->
<div id="wimsmenubox" class="footer">
 <$div0 class="wimsmenu">
  <div class="menublock">
   !if $class_typename notsametext program
    <$div1 class="menuitem SE_list">
      !read ./adm/oneitem.phtml SE_list,1,module=adm/class/userlist
    </$div1><!-- menuitem-->
    <$div1 class="menuitem n_participant">
      !read ./adm/oneitem.phtml n_participant,1,style=student
    </$div1><!-- menuitem-->
   !endif
  </div><!--end menublock-->
  <div class="menublock">
    !read themes/_widgets/supervisormotd.phtml
    
    !if $exist_cdt=yes
      !read themes/_widgets/menuitem.phtml cdt,1,module=adm/class/cdt
    !endif

    !read themes/_widgets/supervisorforum.phtml
  </div><!--end menublock-->
  <div class="menublock">
    <$div1 class="menuitem SE_config">
     !read ./adm/oneitem.phtml SE_config,1,module=adm/class/config
    </$div1><!--menuitem-->
    <$div1 class="menuitem pref">
     !read ./adm/oneitem.phtml pref,1,module=adm/light&phtml=useropts.phtml.$lang
    </$div1><!--menuitem-->
  </div><!--end menublock-->
 </$div0><!--end wimsmenu-->
</div><!--end wimsmenubox-->
<!-- end _widgets/wimsmenubox -->