<!-- begin of $wims_theme/tail.phtml -->
!! Tail menu theme.
!nocache
!set wims_theme_tail=yes
</div><!--end of wimspagebox or wimsbody-->
</div><!--end of wimspagebox or wimsbody-->
!if $wims_homeref_special!=$empty
 </div><!--end of wimspagebox -->
 <div class="wimsmenu"> 
 <div class="tailitem">$wims_homeref_special</div><!--tailitem-->
 </div><!--wimsmenu-->
 </div><!--wimsbodybox-->
</div><!--id wimsexam-->
 !exit
!endif
!if $wims_homeref_n0+$wims_homeref_n4 + $wims_homeref_n5>0
<div id="wimsmenumodubox" style="background-color:$wims_ref_bgcolor;">
 !if $wims_homeref_n0>0
 <div class="wimsmenu">
 !for i_=1 to $wims_homeref_n0
  !set 1_=!line $i_ of $wims_rm_0
  !if $(1_[1])=
   <div class="itemsep"></div>
  !else
   <div class="menuitem">
     !read oneitem.phtml $1_
   </div>
  !endif
 !next i_
</div><!--wimsmenu-->
!endif wims_homeref_n0
!if $wims_homeref_n5>0
<div class="wimsmenu">
!for i_=1 to $wims_homeref_n5
 <span class="menuitem">
 !set 1_=!line $i_ of $wims_rm_5
 !read oneitem.phtml $1_
 </span><!-- menuitem-->
!next i_
</div><!--wimsmenu-->
!endif wims_homeref_n5
!if $wims_homeref_n4>0
<div class="wimstail">
!for i_=1 to $wims_homeref_n4
 <div class="tailitem">
 !set 1_=!line $i_ of $wims_rm_4
 !read oneitem.phtml $1_
 </div><!--tailitem-->
!next i_
</div><!--wimstail-->
!endif wims_homeref_n4
</div>
<!--wimsmenumodubox-->
!endif wims_homeref_n0+wims_homeref_n4+$wims_homeref_n5
!if $wims_homeref_n3>0
  <div id="wimsmenutab">
  <div class="wimsmenu"> 
  !for i_=1 to $wims_homeref_n3
    !set 1_=!line $i_ of $wims_rm_3
    !if $(1_[1])=
      <div class="itemsep"></div>
    !else
     <div class="menuitem">
     !read oneitem.phtml $1_
     </div>
    !endif
  !next i_
  </div><!--wimsmenu-->
  </div><!--wimsmenutab-->
!endif
!if $wims_homeref_n1>0
<div id="wimsmenubox" style="background-color:$wims_ref_bgcolor;">
<div class="wimsmenu">
!for i_=1 to $wims_homeref_n1
 <span class="menuitem">
 !set 1_=!line $i_ of $wims_rm_1
 !read oneitem.phtml $1_
 </span><!-- menuitem-->
!next i_
</div><!--wimsmenu-->
</div><!--wimsmenubox-->
!endif
</div><!--wimsbodybox-->
!if $wims_homeref_n2>0
<div id="wimstailbox">
<div class="wimstail">
!for i_=1 to $wims_homeref_n2
 <div class="tailitem">
 !set 1_=!line $i_ of $wims_rm_2
 !read oneitem.phtml $1_
 </div><!--tailitem-->
!next i_
</div><!--wimstail-->
</div><!--wimstailbox-->
!endif
!endif

!if help isin $wims_session
</div><!--wims_mhelp-->
!endif
!if $wims_lightpopup=yes
 </div> <!--wimslightpopup-->
 </div>
!endif

<!-- end of $wims_theme/tail.phtml -->
