<!-- begin of $wims_theme/tail.phtml -->
!! Tail menu theme.
!nocache
!set wims_theme_tail=yes


<div class="spacer"></div>
</div><!--end of wimsbody-->
 <div class="spacer"></div>
</div><!--end of wimspagebox-->


<!-- MENUMODUBOX (droite) -->

<div id="wimsmenumodubox">

 <!--affiche le menu si le module n'est pas la zone enseignant-->
!if $module != adm/class/classes 

!if $wims_homeref_n0+$wims_homeref_n4 + $wims_homeref_n5 + $wims_homeref_local_n0>0

 !if $wims_homeref_n0>0

 <ul class="wimsmenu">
 
 
 !for i_=1 to $wims_homeref_n0
  !set 1_=!line $i_ of $wims_rm_0
  !if $(1_[1])=
   <li class="itemsep"></li>
  !else
   <li class="menuitem">
     !read oneitem.phtml $1_
   </li>
  !endif
 !next i_
 </ul>

!endif wims_homeref_n0

!if $wims_homeref_n5>0
<ul class="wimsmenu">
!for i_=1 to $wims_homeref_n5
 <li class="menuitem">
 !set 1_=!line $i_ of $wims_rm_5
 !read oneitem.phtml $1_
 </li><!-- menuitem-->
!next i_
</ul><!--wimsmenu-->
!endif wims_homeref_n5

!if $wims_homeref_n4>0
<div class="wimstail">
!for i_=1 to $wims_homeref_n4
 <div class="tailitem">
 !set 1_=!line $i_ of $wims_rm_4
 !read oneitem.phtml $1_
 </div><!--tailitem-->
!next i_
</div><!--wimstail-->
!endif wims_homeref_n4

!if $wims_homeref_local_n0>0
 <ul class="wimsmenu">
 
 !for i_=1 to $wims_homeref_local_n0
  !set 1_=!line $i_ of $wims_local_menu_items
  !if $(1_[1])=
   <li class="itemsep"></li>
  !else
   <li class="menuitem">
     !read oneitem.phtml $1_
   </li>
  !endif
 !next i_
</ul><!--wimsmenu-->
!endif wims_homeref_local_n0

!endif wims_homeref_n0+wims_homeref_n4+$wims_homeref_n5


!endif <!--fin condition si pas page enseignant-->


!if $wims_homeref_special!=$empty
 <ul class="wimsmenu"> 
 <div class="tailitem">$wims_homeref_special</div><!--tailitem-->
 </ul><!--wimsmenu-->
 </div><!--wimsbodybox-->
</div><!--id wimsexam-->
 !exit
!endif

</div>
<!-- FIN WIMSMENUMODUBOX-->


<!-- MENU BAS DE PAGE -->

!if $wims_homeref_n1>0
<div id="wimsmenubox" class="footer">
<ul class="wimsmenu">
!for i_=1 to $wims_homeref_n1
 !set 1_=!line $i_ of $wims_rm_1
 !set css_class=!item 1 of $1_
 <li class="menuitem $css_class">
 !read oneitem.phtml $1_
 </li><!-- menuitem-->
!next i_
</ul><!--wimsmenu--></div><!--wimsmenubox-->
!endif


<div class="spacer"></div>
</div>
<!-- FIN WIMSBODYBOX-->



<!--BAS DE PAGE-->

!if $wims_homeref_n2>0
<div id="wimstailbox">
<div class="wimstail">
!for i_=1 to $wims_homeref_n2
 <div class="tailitem">
 !set 1_=!line $i_ of $wims_rm_2
 !read oneitem.phtml $1_
 </div><!--tailitem-->
!next i_
</div><!--wimstail-->

</div><!--wimstailbox-->
!endif
!endif

<!--FIN BAS DE PAGE-->

!if help isin $wims_session
</div><!--wims_mhelp-->
!endif
!if $wims_lightpopup=yes
 </div> <!--wimslightpopup-->
 </div>
!endif

<!-- end of $wims_theme/tail.phtml -->
