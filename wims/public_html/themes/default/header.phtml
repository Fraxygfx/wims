!! Header menu bar definition.
!nocache
!set wims_theme_header=yes

<div class=wimsmenu>
<center><table border=0 cellspacing=0 cellpadding=2 bgcolor=$wims_name_bgcolor
width=100%><tr>
!for i_=1 to $wims_hm_cnt
 <td align=center>
 !set 1_=!line $i_ of $wims_hm
 !read oneitem.phtml $1_
 </td>
!next i_

</tr></table></center>
</div>
