!! document menu
!nocache
!set wims_theme_docheader=yes

<table width="100%" border="0" cellspacing="0" cellpadding="3" bgcolor="$wims_ref_bgcolor">
<tr><td align="left"><small>&nbsp;
!if help notin $wims_session
 !if $wims_class=$empty
  !href module=home $doch_wims
 !else
  !if _doc isin $wims_session
   <a href="#" onclick="window.close()">$doch_class</a>
  !else 
   !href module=home $doch_class
  !endif
 !endif
&nbsp;
 !if ..//adm/doc isin ..//$module and \
    ($wims_class=$empty or $wims_user=supervisor)
  !href module=$module&cmd=reply&job=home $doch_docs
 !endif
!endif
!default upbl=main
!if $block!=main and $job=read
 &nbsp;
 !href cmd=reply&doc=$doc&block=$upbl $doch_up
 !! <img src="gifs/arrows/up2.32.gif">
 !if $prev!=$empty and $prev!=$upbl
  &nbsp;
  !href cmd=reply&doc=$doc&block=$prev $doch_prev
 !endif
 !if $next!=$empty and $next!=$upbl
  &nbsp;
  !href cmd=reply&doc=$doc&block=$next $doch_next
 !endif
!endif
!if $job=read
 !if $blocklast$blocknext!=$empty
  &nbsp;[
  !if $blocklast!=$empty
   !href cmd=reply&doc=$doclast&block=$blocklast&readcntadv=-1 &lt;&lt;
  !endif
   &nbsp;$doch_hist
  !if $blocknext!=$empty
   &nbsp;
   !href cmd=reply&doc=$docnext&block=$blocknext&readcntadv=1 &gt;&gt;
  !endif
  ]
 !endif
!! &nbsp;
!! !href cmd=reply&job=read $doch_reload
!endif
<td align="right"><small>
!if $printable=yes
 !href cmd=reply&job=read&doc=$doc&block=$startblock $doch_interactive
!else
 !if $job=read
  !href cmd=reply&job=print&doc=$doc $doch_printable
&nbsp;
 !endif
 !if $job=exosource
  !href cmd=reply&job=read $doch_back
&nbsp;
 !endif
 !if $doc!=$helpd and $editable=yes
  !href target=wims_help module=$module&cmd=new&job=read&doc=$helpd $doch_help
&nbsp;
 !endif
 !href target=wims_mhelp module=$module&cmd=help&special_parm=about $doch_about
&nbsp;
!endif
</table>
