!goto $wims_read_parm

:def
title=Moyenne quadratique
input=data1d
!exit

:proc

cnt=!itemcnt $formula
!if $cnt>0
 result=!sum x^2 for x in $formula
 result=$[sqrt(($result)/$cnt)]
!else
 result=0
!endif

!exit

:output

<center>
Moyenne quadratique =
!htmlmath $result
</center> <p>
Données : $cnt nombres { $formula }
<p>
La <em>moyenne quadratique</em> de {<i>x</i><sub>1</sub>,
<i>x</i><sub>2</sub>, ..., <i>x<sub>n</sub></i>} est égale à
!set ins_align=middle
!instex $$$$ \sqrt{{1\over n}\sum_{i=1}^n x_i^2} $$$$
&nbsp;.
!exit

