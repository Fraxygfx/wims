!goto $wims_read_parm

:def
title=Nombre complexe
synonyme=argument, partie reelle, partie imaginaire
input=function
!exit

:proc
result=!exec pari c=$formula; print(real(c));\
	print(imag(c));\
	print(abs(c));\
	print(arg(c));\
	print(arg(c)/pi);
result=!nonempty lines $result
n=!linecnt $result
!if $n<5
 result=
!endif
!distribute lines $result into re,im,mod,arg,argp
!exit

:output
Nombre complexe
!htmlmath c = $formula
&nbsp;:
<table class="wimscenter wimsnoborder">
 <tr><td>Re(c) = </td><td> $re</td></tr>
 <tr><td>Im(c) = </td><td> $im</td></tr>
 <tr><td>|c| = </td><td> $mod</td></tr>
 <tr><td>argument(c) = </td><td>$arg + 2k$m_pi</td></tr>
 <tr><td> = </td><td>$argp$m_pi + 2k$m_pi</td></tr>
</table>
!exit
