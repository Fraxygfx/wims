!goto $wims_read_parm

:def
title=Harmonic mean
input=data1d
!exit

:proc

cnt=!itemcnt $formula
!if $cnt>0
 result=!sum 1/x for x in $formula
 result=$[$cnt/($result)]
!else
 result=0
!endif

!exit

:output

<center>
Harmonic mean =
!htmlmath $result
</center> <p>
Data: $cnt numbers { $formula }
<p>
The <em>harmonic mean</em> of {<i>x</i><sub>1</sub>,
<i>x</i><sub>2</sub>, ..., <i>x<sub>n</sub></i>} is equal to
!set ins_align=middle
!instex $$$$ n\over \sum_{i=1}^n {1\over x_i} $$$$
&nbsp;.
!exit

