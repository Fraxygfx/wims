typ6=fset
typ7=vector
typ8=function
typ9=algexp
typ10=formal
typ11=matrix

typafList=
ftypList=
!for i=1 to 11
  typafList=!append line $(af$(i)) to $typafList
  ftypList=!append line $(typ$(i)) to $ftypList
!next i


