!if $modifcnt+$newcnt<=0
  <p>
  No module publication found corresponding to your search.
  !exit
!endif

Click on the address of a module to check/install/update it.
<p>
!if $modifcnt>0
 $table_header
 <caption>Found new versions &nbsp;
 !href cmd=reply&job2=all&muauto=update update all
 </caption>
 $table_hdtr<th>$wims_name_Address
 <th><small>updated<br>by</small>
 <th><small>New<br>version</small>
 <th><small>Server<br>version</small>
 !for i=1 to $modifcnt
  
  !set l=!line $i of $modif
  !distribute items $l into w_,a_,v_,t_
  $table_tr<td>
  !href cmd=reply&job2=one&muauto=check&mupack=$w_~$a_ $a_
  <td align=center>$t_<td align=center>$v_
  <td align=center>
  !module version $a_
  </tr>
 !next i
 $table_end <p>
!endif

!if $newcnt>0
 $table_header
 <caption>Found new modules &nbsp;
 !href cmd=reply&job2=all&muauto=install install all
 </caption>
 $table_hdtr<th>$wims_name_Address
 <th><small>published<br>by</small>
 <th><small>$wims_name_Version</small>
 !for i=1 to $newcnt
  
  !set l=!line $i of $new
  !distribute items $l into w_,a_,v_,t_
  $table_tr<td>
  !href cmd=reply&job2=one&muauto=check&mupack=$w_~$a_ $a_
  <td align=center>$t_<td align=center>$v_
  </tr>
 !next i
 $table_end <p>
!endif

