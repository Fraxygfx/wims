
<center><h3>Current server activities</h3></center>

<p>Server Time: $nowtime.
!if $load15!=$empty
 Load average: $load1% (last 1 minute), $load5% (last 5 minutes),
 $load15% (last 15 minutes).
!endif
<p>$table_header
<caption>Number of requests</caption>
$table_hdtr
<th>since 10 min</th>
<th>since 30 min</th>
<th>since 1 hour</th>
<th>since 3 hours</th></tr>
$table_tr
<td align=center>$access10m</td>
<td align=center>$access30m</td>
<td align=center>$access1h</td>
<td align=center>$access3h</td></tr>
$table_end

<p><a name=sessions>
$table_header
<caption>Active sessions within last 10 minutes
<small>[
!href cmd=reply#sessions refresh
 ]</small>
</caption>
$table_hdtr<th>#</th>
<th>Session</th><th>Client IP</th>
<th>Requests</th>
<th>Module</th><th>Class</th></tr>
!set n=!linecnt $ses
!set k=1
!for i=1 to $n
 !set l=!line $i of $ses
 !distribute words $l into r_,s_,i_,m_,c_
 !if o notin $s_ and - notin $s_
  $table_tr
  <td align=right>$k</td>
  <td><small><tt>
  !set v_=!charcnt $s_
  !if $v_>6
   !href cmd=reply&job=sess&checkses=$s_ $s_
  !else
   $s_
  !endif
</tt></small></td>
  <td><tt>$i_</tt></td>
  <td align=center>$r_</td>
  <td>$m_</td>
  !if , isin $c_
   <td>$c_</td>
  !else
   <td>--</td>
  !endif
  </tr>
  !advance k
 !endif
!next i
$table_end <p>


