!!###language independant
!read adm/css.phtml
!header

!if $error!=$empty
 !read msg.phtml
 <p><hr> <p>
!endif
!read $job.phtml

!if $auth_login!=$empty and $job!=modname
 <p>
 !if $job iswordof flist edfile showfile binfile
 !set wims_menu_tail=!append line test,4,module=devel/$auth_login/$mod,test\
 to $wims_menu_tail
  !set doctest=!module category devel/$auth_login/$mod
  !if document isin $doctest
    !set wims_menu_tail=!append line docgestion,4,module=adm/doc.$module_language&devdoc=$mod&doc=1&job=read,test\
     to $wims_menu_tail
  !endif
 !endif
 !if $job iswordof flist index publish prop diff binfile
  !if $job iswordof index publish diff
   !set wims_menu_tail=!append line modcontent,1,cmd=reply&jobreq=flist\
   binfile,1,cmd=reply&jobreq=binfile\
   to $wims_menu_tail 
  !endif
  !if $job iswordof binfile
   !set wims_menu_tail=!append line otherfiles,1,cmd=reply&jobreq=flist\
     to $wims_menu_tail
  !endif
  !if $job iswordof flist publish diff binfile
  !set wims_menu_tail=!append line properties,1,cmd=reply&jobreq=index\
  to $wims_menu_tail
  !endif
  !if $job notwordof publish prop diff
  !set wims_menu_tail=!append line modsave,1,cmd=reply&jobreq=backup\
  to $wims_menu_tail
   !if $pubtit!=$empty
   !set wims_menu_tail=!append line checkdiff,1,cmd=reply&jobreq=diff\
    to $wims_menu_tail
   !endif
   !set wims_menu_tail=!append line publish,1,cmd=reply&jobreq=publish\
   to $wims_menu_tail
  !endif
 !endif
 !set wims_menu_tail=!append line modlist,1,cmd=reply&jobreq=modname\
 to $wims_menu_tail
!endif

!tail

!reset confirm target1 target2 target3 target4 vcheat \
      i_translator i_translator_address, modifiers \
      p_name p_email p_pass1 p_pass2 bin_job
