!read adm/title.phtml 3\
$title

!if $parm=$empty
 $name_domain
$table_header
 !for t in $Tot_dom
 $table_tr <th colspan="2">
 !lookup $t in $dir_lang
 </th></tr>
  !for d in $(tot_dom_$t)
  $table_tr<td>
   !set l_=$(dom_$d)
   !set t_=!lookup $d in $dir_lang
    !href module=$module&cmd=new&job=$job&parm=$d <b>$t_</b>
    </td>
   !if $l_=$empty
    <td>
   !else
    <td>
    !for v in $l_
    !set t_=!lookup $v in $dir_lang
     !href module=$module&cmd=new&job=$job&parm=$v $t_
     &nbsp;
    !next v
   !endif
   </td>
   </tr>
  !next d
 
 !next $t
 </tr>
 $table_end
!else
 $name_available
!set wims_menu_items=!append line other,1,module=$module&cmd=new&job=subject\
  to $wims_menu_items
 $table_header
 !for t in $list
  $table_tr
  !!keep key - use in search_addr.
  !reset key
    !set key=!lookup $t in $dir
    !set key_l=!lookup $t in $dir_lang
    !set name_$t=!lookup $t in $dir_lang
    !set key=!replace internal _ by $ $  in $t
    !set key=$key,$key_l
    <td align="left" nowrap="nowrap"><b>$(name_$t)</b></td>
  !for i in A,T,P,X,S
   !set cat=$i
   <td nowrap="nowrap">
   !if $i!=A
   <span class="small">
   !href $search_addr $(name_$i)
   </span>
   !else
   !href $search_addr $(name_$i)
   !endif
  </td>
  !next i
 </tr>
  !next t
 </table>
!endif

