!read adm/title.phtml 3\
$title

!if $parm=$empty
 $name_domain
$table_header
 !for t in $Tot_dom
 $table_tr <th colspan="2">$(name_$t)</th></tr>
  !for d in $(tot_dom_$t)
  $table_tr<td>
   !set l_=$(dom_$d)
    !href module=$module&cmd=new&job=$job&parm=$d <b>$(name_$d)</b>
    </td>
   !if $l_=$empty
    <td>
   !else
    <td>
    !for v in $l_
     !href module=$module&cmd=new&job=$job&parm=$v $(name_$v)
     &nbsp;
    !next v
   !endif
   </td>
   </tr>
  !next d
 
 !next $t
 </tr>
 $table_end
!else
 $name_available
!set wims_menu_items=!append line other,1,module=$module&cmd=new&job=subject\
  to $wims_menu_items
 $table_header
 !for t in $list
  $table_tr
    !set key=$(keywords_$t)
    !default name_$t=!replace internal _ by $ $  in $t
    !default key=!replace internal _ by $ $  in $t
  <td align="left" nowrap="nowrap"><b>$(name_$t)</b></td>
  !for i in A,T,P,X,S
   !set cat=$i
   <td nowrap="nowrap">
   !if $i!=A
   <span class="small">
   !href $search_addr $(name_$i)
   </span>
   !else
   !href $search_addr $(name_$i)
   !endif
  </td>
  !next i
</tr>
  !next t
 </table>
!endif

