!read adm/title.phtml 2\
$title

<div class="wims_content">


<div class="job_desc spacer">$job_desc</div>
!set cnt=0
<div class="wims_credits">
</div>
  <div id="taxonomy_list">
    <ul class="tabs">
      !for taxo in $list_taxonomy
       !increase cnt
        !set taxo_title$cnt=!defof title in $taxo
        !set desctaxo$cnt=!defof desctaxo in $taxo
        <li><a href="#$taxo">$(taxo_title$cnt)</a></li>
      !next
    </ul>
    !for taxo in $list_taxonomy
      <div id="$taxo">
         !read $taxo
       </div>
    !next
  </div>
</div>
 <!-- JS for dynamic tree display -->
<script type="text/javascript" src="scripts/js/tree_JS/tree.js"></script>
 <!-- JS for Tabs display -->
<script type="text/javascript">
    /*<![CDATA[*/
    jQuery(function($$) {
        $$( "#taxonomy_list" ).tabs();
      });
    /*]]>*/
</script>

<div class="wims_credits">
<h2>$wims_name_credits</h2>
!reset cnt
!for taxo in $list_taxonomy
 !increase cnt
 <p>
  $(taxo_title$cnt) : $(desctaxo$cnt)
 </p>
!next
</div>
