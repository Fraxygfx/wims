!set wims_name_adddevel=Añadir una nueva cuenta de desarrollador
!set wims_name_develaccount=Lista de cuentas de desarrolladores

!if $job2 iswordof mod1 mod2 del1
 !read developer/$job2.phtml
 !set wims_menu_items=!append line develaccount,1,cmd=reply&job2=first\
to $wims_menu_items
 !exit
!endif

<p>$table_header
<caption>Desarrolladores de módulos registrados en este servidor</caption>
$table_hdtr
<th>$wims_name_Login<th>$wims_name_name<th>$wims_name_action
!for d=1 to $devcnt
 !distribute lines $(drec$d) into login,passwd,name,email
 $table_tr
 <td align=center>$login
 <td align=center>
 <a href="mailto:$email">$name</a>
 <td nowrap><small>
 !href cmd=reply&job2=del1&devno=$d $wims_name_erase
&nbsp;
 !href cmd=reply&job2=mod1&devno=$d $wims_name_properties
&nbsp;
 !href module=adm/modtool&login=$login&passwd=$passwd&jobreq=modname Visitar
</small>
!next d
$table_end <p>

!set wims_menu_items=!append line adddevel,1,cmd=reply&job2=mod1&devno=0\
to $wims_menu_items

!form reply
<input type=hidden name=job2 value=pub>
Desarrolladores a los que se les permite instalar módulos en el subdirectorio
local del sitio (escriba sus identificadores de usuario): <br>
<input size=50 name=devpublisher value="$devpublisher">
<input type=submit value="$wims_name_tosave">
</form>

!reset job2,devno,devlogin,devpasswd,devname,devemail,devpublisher

