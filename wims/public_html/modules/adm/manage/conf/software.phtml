
!if $save!=$empty and $changed>0
 !href cmd=reply#check $name_click
$name_check
 <p>
 !exit
!endif

<p><hr> <p>
<a name="check"></a>
$name_checksoftware (
!href cmd=reply&confcat=5#check $name_click
 $name_checkgraphics.)

<p>$table_header
$table_hdtr
<th>$wims_name_name<th>$name_command<th><small>$name_importance</small><th>.<th>$name_availability

!distribute items PARI/GP,Maxima,MuPAD,GAP,Octave,\
	Macaulay 2,Yacas\
	into n_pari,n_maxima,n_mupad,n_gap,n_octave,\
	n_m2,n_yacas
!distribute items 100,80,0,10,20,\
	5,20\
	into i_pari,i_maxima,i_mupad,i_gap,i_octave,\
	i_m2,i_yacas
!distribute items 2^17-1,2^17-1;,2^17-1;,2^17-1;,2^17-1,\
	2^17-1,2^17-1\
	into t_pari,t_maxima,t_mupad,t_gap,t_octave,\
	t_m2,t_yacas
!set res=131071

!for sw in pari,maxima,yacas,gap,m2,octave
 
 $table_tr<td align="center">$(n_$sw)<td align="center">$sw
 <td align="center">$(i_$sw)%
 <td align="center"><small>
 !href cmd=help&help_subject=soft&help_conf=$sw $wims_name_help
 !set av=!exec $sw about
 !set av=!trim $av
 !set r_=!exec $sw $(t_$sw)
 !set r_=!trim $r_
 !if $sw=m2
  !set r_=!word 3 of $r_
 !endif
</small><td>
 !if $av!=$empty and $res=$r_
  $wims_name_yes: $av
 !else
  $name_Unavailable.
 !endif
!next sw

$table_end <p>

!if $othermiss!=$empty
 $name_warning_program
 !set ll=!record 12 of lang/cat.$modu_lang
 !set ll=!line 2 of $ll
 !href cmd=reply&job=conf&confcat=12 $ll
).
 <p>$table_header
 $table_hdtr<th>$wims_name_name<th>$name_dependingonit
 !for other in $othermiss
  
  !set o=!replace internal units-filter by units in $other
  $table_tr<td><tt>$other</tt>
  <td>
  !if $(mod_$o)!=$empty
   !set mn=!module title $(mod_$o)
   !if $mn!=$empty
    !href module=$(mod_$o) $mn
   !endif
  !else
   Lots of modules
  !endif
  !if $other iswordof chemeq units-filter
   <small>$name_fail.</small>
  !endif
  </td></tr>
 !next other
 $table_end <p>
!endif

