
!read adm/title.phtml 3\
\
当前服务器的活动

<p>服务器时间: $nowtime.
!if $load15!=$empty
 平均载入量: $load1% (最近一分钟), $load5% (最近5分钟),
 $load15% (最近15分钟).
!endif
<p>$table_header
<caption>请求数</caption>
$table_hdtr
<th>10分钟以内</th>
<th>30分钟以内</th>
<th>1小时以内</th>
<th>3小时以内</th></tr>
$table_tr
<td align=center>$access10m</td>
<td align=center>$access30m</td>
<td align=center>$access1h</td>
<td align=center>$access3h</td></tr>
$table_end

<p><a name=sessions></a>
$table_header
<caption>最近10分钟内活动的会话
<small>[
!href cmd=reply#sessions $wims_name_refresh
 ]</small>
</caption>
$table_hdtr<th>#</th>
<th>会话</th><th>客户IP</th>
<th>请求</th>
<th>模块</th><th>班级</th></tr>
!set n=!linecnt $ses
!set k=1
!for i=1 to $n
 !set l=!line $i of $ses
 !distribute words $l into r_,s_,i_,m_,c_
 !if o notin $s_ and - notin $s_
  $table_tr
  <td align=right>$k</td>
  <td><small><tt>
  !set v_=!charcnt $s_
  !if $v_>6
   !href cmd=reply&job=sess&checkses=$s_ $s_
  !else
   $s_
  !endif
</tt></small></td>
  <td><tt>$i_</tt></td>
  <td align=center>$r_</td>
  <td>$m_</td>
  !if , isin $c_
   <td>$c_</td>
  !else
   <td>--</td>
  !endif
  </tr>
  !advance k
 !endif
!next i
$table_end <p>


