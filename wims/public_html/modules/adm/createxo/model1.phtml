
!readproc model/makelist.proc

$name_availablemodels
<p>
!for t in $types
 !default name_$t=$t
 $table_header
 <caption>$(name_$t)</caption>
 $table_hdtr
 <th>$wims_name_Type</th>
 <th>$wims_name_Action</th>
 <th>$wims_name_Description</th>
 </tr>
 !for f in $(type_$t)
  !set r=!record 1 of model/src/$f.oef
  !set r=!line 1 of $r
  $table_tr
  <td>$(tit_$f)</td>
  <td nowrap>
  !href cmd=resume&level=3&mode=model&model=$f&realtest=yes $name_demo
&nbsp;
  !href cmd=reply&level=2&mode=model&model=$f $name_adapt
  </td>
  <td><small>$(desc_$f)
  !href target=wims_mhelp cmd=help&special_parm=model,1,$f &gt;&gt;
  </small></td>
  </tr>
 !next f
 $table_end
 <p>
!next t

!read lang/names.phtml.$modu_lang model

!reset mode

