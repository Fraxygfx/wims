!read lang/names.phtml.$modu_lang intro

!form reply
 !set s_day=$name_day
 !set s_week=$name_week
 !set s_month=$name_month
 !set s_year=$name_year
 !read adm/date.phtml date,$start_year,$start_month,$start_day
 !set s_beginning=$name_beginning $c_date_out
$name_choose
 <select name="cnt">
 !for i=1 to 15
  <option value=$i
  !if $i=$cnt
   selected="selected"
  !endif
  >$i</option>
 !next i
 </select><select name="style">
 !for i in day,week,month,year,beginning
  <option value="$i"
  !if $i=$style or ($i=month and $style=$empty)
   selected="selected"
  !endif
  >$(s_$i)</option>
 !next i
 </select>. <p>
 $name_listlink
 !set i_site=$name_site1,
 !set i_stem=$name_stem, $wims_name_or
 !set i_uniq=$name_uniq
 !for i in site,stem,uniq
  <input type="radio" name="type" value="$i"
  !if $type=$i
   checked="checked"
  !endif
  /> $(i_$i)</option>
 !next i
 $name_limited <select name="reflim">
 !for i in 10,20,30,50,70,100,150,200
  <option value="$i"
  !if $i=$reflim
   selected="selected"
  !endif
  >$i</option>
 !next i
 </select>$name_searchengine <input type="radio" name="search" value="yes"
 !if $search=yes
  checked="checked"
 !endif
 /> $name_included <input type="radio" name="search" value="no"
 !if $search=no
  checked="checked"
 !endif
 /> $name_excluded.
 <p>$name_option
 <input size="15" name="key" value="$key"/>
 </p>
 <p class="wimscenter"><input type="submit" value="$wims_name_Show"/></p>
</form>

!set key=

