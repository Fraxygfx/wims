 !for j=1 to $sheetcnt
 !if $(activ[$j])>0
  <th colspan=2><small>$name_sheet $j <br>($(nbexo$j) $name_exo)</small></th>
 !endif
 !next j
 </tr>
 $table_hdtr
 <th></th><th></th><th></th><th></th><th></th>
