!if $exologcnt<1
 No se ha grabado ningún ejercicio de $wims_firstname $wims_lastname.
 !exit
!endif

Estos son los ejercicios grabados de $wims_firstname $wims_lastname.
Pulse en la fecha para reproducir uno.
!read colors-class

<p><center><table border=0 cellspacing=2 cellpadding=1>
<tr bgcolor=$table_bgcolor>
<th><small>Fecha de la grabación</small></th>
<th><small>Hoja</small></th>
<th><small>Ejercicio</small></th>
<th><small>Pasos</small></th>
<th><small>Puntuación</small></th>
<th>Título</th>
!for i=1 to $exologcnt
 !set l=!line $i of $exolog2
 !set l_=!line $i of $exologs
 !distribute item $l into d_,s_,n_
 !set n_=!translate . to , in $n_
 !distribute item $n_ into S_,E_
 !set s_=!getdef w_module_score in classes/$wims_class/exolog/$checkuser/$l_
 !default s_=-
 !set p_=!recordcnt classes/$wims_class/exolog/$checkuser/$l_
 !set V_=!record $E_ of classes/$wims_class/sheets/.sheet$S_
 !distribute lines $V_ into a,a,a,a,T_,D_
 !if $D_!=$empty
  !exchange table_color1,table_color2
  <tr bgcolor=$table_color2><td align=center>
  !href target=wims_check module=$module&job=check&checkuser=$checkuser&logcheck=$l_&checkstep=1 $d_
  </td>
  <td align=center>$S_</td><td align=center>$E_</td>
  <td align=center>$p_</td>
  <td align=center>$s_</td>
  <td>$T_ <small>$D_
  !if $wims_user=supervisor
   !getfile exolog/$l_ source
  !endif
  </small></td></tr>
 !endif
!next i
</table></center>

