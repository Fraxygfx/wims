!for ld_ in cas_auth,ldap_auth,ldap_port,ldap_version,ldap_base,ldap_branch,ldap_uid,ldap_login
  !default up_$ld_=!defof $ld_ in wimshome/log/wims.conf
!next

!if  $up_auth=ldap
 !if $up_ldap_auth issametext  or $up_ldap_branch issametext or $up_ldap_base issametext
  error=no_ldap_parameter
  !exit
 !endif
 
 !default up_ldap_login=supannAliasLogin
 !default up_ldap_uid=uid
 !default up_ldap_port=389

 !setdef !set class_auth=$up_auth\
!set class_ldap_auth=$up_ldap_auth\
!set class_ldap_port=$up_ldap_port\
!set class_ldap_version=$up_ldap_version\
!set class_ldap_base=$up_ldap_base\
!set class_ldap_branch=$up_ldap_branch\
!set class_ldap_uid=$up_ldap_uid\
!set class_ldap_login=$up_ldap_login\
in wimshome/log/classes/$wims_class/.def

!else
  !setdef !set class_auth=\
!set class_ldap_auth=$up_ldap_auth\
!set class_ldap_port=$up_ldap_port\
!set class_ldap_version=$up_ldap_version\
!set class_ldap_base=$up_ldap_base\
!set class_ldap_branch=$up_ldap_branch\
!set class_ldap_uid=$up_ldap_uid\
!set class_ldap_login=$up_ldap_login\
in wimshome/log/classes/$wims_class/.def
!endif

!if  $up_auth=cas
  !setdef !set class_auth=cas\
!set class_cas_auth=$up_cas_auth\
in wimshome/log/classes/$wims_class/.def
!else
  !setdef !set class_cas_auth=$up_cas_auth\
in wimshome/log/classes/$wims_class/.def

!endif
  


