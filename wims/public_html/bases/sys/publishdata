!if $wims_read_parm iswordof wimsedu unice
  !goto $wims_read_parm
!else
  !goto unice
!endif

:wimsedu
publishname=WIMS EDU Publication Center
sysadmin=wims@wimsedu.info
domainlist=algebra, analysis, arithmetics, geometry, probability
levellist=E1,E2,E3,E4,E5,E6,H1,H2,H3,H4,H5,H6,U1,U2,U3,U4,U5,Lang
zonelist=test,$levellist,tool,data,local,help
zonepublish=$levellist,tool,data,local,help
centralcgi=wims/wims.cgi

centralhost=publishcenter.wimsedu.info
collector=adm/publish/collector
!exit

:unice
