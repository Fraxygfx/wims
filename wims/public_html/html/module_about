   <tr><td>Opis</td><td>$module_description</td></tr>\
   <tr><td>Verzija</td>    <td>$module_version</td></tr>\
   <tr><td>Jezik</td>   <td>slovenski</td></tr>\
   <tr><td>Avtor</td>    <td>$module_au</a></td></tr>\
   $module_about_tr\
   <tr><td>Avtorske pravice</td> <td>$module_copyright</td></tr>\
   </table></center>
   !exit
!endif

